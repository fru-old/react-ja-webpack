{
  "name": "react-es6-webpack-boilerplate",
  "version": "4.0.0",
  "description": "Boilerplate for kick starting a React Project with ES6 (Babel) and Hot reloader using Webpack.",
  "main": "index.js",
  "scripts": {
    "start": "node -e require('./tools/webpack.conf.js')('dev')",
    "build": "node -e require('./tools/webpack.conf.js')('prod')",
    "lint": "./node_modules/.bin/eslint . || exit 0",
    "test": "node ./node_modules/karma/bin/karma start ./tools/karma.conf.js --browsers PhantomJS",
    "css": "casperjs --web-security=no --ssl-protocol=any --ignore-ssl-errors=yes test ./test/css/casper.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vasanthk/react-es6-webpack-boilerplate.git"
  },
  "keywords": [
    "react",
    "es6",
    "babel",
    "webpack"
  ],
